public class learn
{
	public static void main(String[] args) {
		int n=5;
		for(int i=2;i<n/2;++i)
		{
			if(n%i==0)
			{
				//System.out.println("print number is prime" +n);
					System.out.println("print number is not prime"+n);
			}
		}
	}
}