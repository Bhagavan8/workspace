public class learn {

    public static void main(String[] args) {

        char c = 'c';
        char c1='A';

        if( (c >= 'a' && c <= 'z') || (c >= 'A' && c <= 'Z'))
    	
        	System.out.println(c + " is an alphabet.");
        else
            System.out.println(c + " is not an alphabet.");
    
    if(c1>='a' && c<= 'z')
    	System.out.println(c1+ "is a char");
    else
    	System.out.println(c1+ " is not char");
    }
    	
}